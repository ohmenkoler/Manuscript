@article{hireche_numerical_2019,
	title = {Numerical study of the effects of natural convection in a thermoacoustic configuration - Natural convection in thermoacoustics},
	volume = {20},
	rights = {© O. Hireche et al., published by {EDP} Sciences 2020},
	issn = {2257-7777, 2257-7750},
	url = {https://www.mechanics-industry.org/articles/meca/abs/2019/08/mi190311/mi190311.html},
	doi = {10.1051/meca/2020051},
	abstract = {This study focuses on natural convection flows within a cylindrical guide containing a porous medium. This configuration is applicable to standing-wave thermoacoustic engines, usually composed of an acoustic resonator where a (short) stack (or porous medium) is inserted, with a heat exchanger placed at one of its ends. The resulting horizontal temperature gradient, when high enough, triggers the onset of an acoustic wave. Natural convection effects are usually neglected in thermoacoustics so that axisymmetry is often assumed. Here a 3D numerical study of natural convection flow is performed using a finite volume code for solving mixed Navier-Stokes and Darcy-Brinkman equations under Boussinesq approximation. The influence of the porous medium’s physical characteristics (permeability, thermal conductivity, anisotropy) on the flow and temperature fields is investigated. It is shown that such flows are fully three-dimensional and therefore can modify significantly starting as well as steady operating conditions of the thermoacoustic engine.},
	pages = {807},
	number = {8},
	journaltitle = {Mechanics \& Industry},
