	number = {6},
	journaltitle = {Applied Acoustics},
	shortjournal = {Applied Acoustics},
	author = {Poignand, Gaëlle and Lihoreau, Bertrand and Lotton, Pierrick and Gaviot, Etienne and Bruneau, Michel and Gusev, Vitaly},
	urldate = {2025-02-28},
