@thesis{bailliet_machines_1998,
	location = {Le Mans, France},
	title = {Machines thermoacoustiques : études analytiques et experimentales de couplages et d'effets non-lineaires acoustiques},
	rights = {Licence Etalab},
	url = {https://www.theses.fr/fr/1998LEMA1013},
	shorttitle = {Machines thermoacoustiques},
	abstract = {Les machines thermoacoustiques font, depuis plus de 15 ans, l'objet d'études spécifiques, principalement aux Etats-Unis. Un certain nombre de systèmes ont été réalisés en laboratoire. Les équations fondamentales de la thermoacoustique sont connues mais restent d'une telle complexite que leur solution n'est obtenue que de facon numérique. L'objet de ce travail est d'entrer dans l'intelligence de ces problèmes, d'en traiter certains aspects sur la base de modèles analytiques et d'orienter les études expérimentales vers la conception et la réalisation de maquettes. Un premier aspect concerne le système couplé source-sonore/resonateur-thermoacoustique pour le réfrigerateur à onde stationnaire. L'objectif est d'aider au choix des paramètres de ce système qui optimisent l'efficacité pour le transfert de chaleur de la source froide à la source chaude. Une modélisation du système couplé, mise en place et validée par un ensemble de résultats expérimentaux, est utilisée pour déterminer la succession d'étapes suivies dans la réalisation d'une maquette. Des expériences de mesure de vitesses particulaires par anémometrie laser-doppler sont effectuées sur cette maquette. Couplées a celle de la pression acoustique ce type de mesure permet d'accéder à une mesure de la puissance acoustique en résonateur thermoacoustique. L'étude de certains phénomènes non-linéaires en résonateur thermoacoustique est ensuite abordée. Deux modèles analytiques sont présentés, dans lesquels la description des effets non-linéaires dans les machines thermoacoustiques permet une meilleure compréhension des phénomènes physiques les régissant. Le premier aboutit à une méthode active de suppression d'harmoniques crées par les non-linéarités qui utilise les caractéristiques du signal appliqué à la source sonore dans un réfrigerateur thermoacoustique. Le second permet de décrire la génération, l'amplification et la saturation en amplitude de l'onde acoustique dans un moteur thermoacoustique.},
	pagetotal = {156},
	institution = {Université du Maine},
	type = {Thèse de doctorat},
	author = {Bailliet, Hélène},
	editora = {Bruneau, Michel},
	editoratype = {collaborator},
	urldate = {2022-05-09},
	date = {1998},
	keywords = {Thermoacoustics, Refrigerator, Cavity resonator, Doppler laser anemometer, Etude experimentale, Experimental study, Modeling, Non linear acoustics},
	file = {Bailliet - 1998 - Machines thermoacoustiques  études analytiques et.pdf:C\:\\Users\\mfontbon\\ownCloud - FONTBONNE Martin@mycore.core-cloud.net\\Zotero\\storage\\7YGHEWPX\\Bailliet - 1998 - Machines thermoacoustiques  études analytiques et.pdf:application/pdf},
